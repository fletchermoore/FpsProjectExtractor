<Window x:Class="FpsProjectExtractor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FpsProjectExtractor"
        mc:Ignorable="d"
        Title="MainWindow" Height="629" Width="745">
    <Grid Margin="10,10,10,10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="332*"/>
            <ColumnDefinition Width="393*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="RunBtn" Width="125"
Height="25" HorizontalAlignment="Left" Click="DoIt" Content="Run" VerticalAlignment="Center"/>
        <Button x:Name="TestBtn" Width="125" Height="25" Content="Test" Click="Test" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="144,0,0,0"/>

    </Grid>
</Window>
